<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  id="Definitions_1"
                  targetNamespace="http://camunda.org/examples">

  <bpmn:process id="task_validation_process" name="Task Validation Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start" />

    <bpmn:userTask id="UserTask_Validation" name="Validate Task" camunda:assignee="demo" />

    <bpmn:endEvent id="EndEvent_1" name="End" />

    <bpmn:sequenceFlow id="flow1" sourceRef="StartEvent_1" targetRef="UserTask_Validation" />
    <bpmn:sequenceFlow id="flow2" sourceRef="UserTask_Validation" targetRef="EndEvent_1" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_task_validation">
    <bpmndi:BPMNPlane id="BPMNPlane_task_validation" bpmnElement="task_validation_process"/>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
